FROM couchbase:7.6.6

EXPOSE 8091-8097 9123 11207 11210 11280 18091-18097

# --- 
# To build this image, run:
# docker build -t cb-server cluster/
# ---

# ---
# To run a Couchbase container with the default name, run:
# docker run -d --name default_db \
#   -p 8091-8097:8091-8097 \
#   -p 9123:9123 \
#   -p 11207:11207 \
#   -p 11210:11210 \
#   -p 11280:11280 \
#   -p 18091-18097:18091-18097 \
#   cb-server
# ---